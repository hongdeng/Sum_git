<?xml version="1.0" encoding="utf-8"?>
<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
 
  <!-- Define some common variables -->
  <PropertyGroup>
    <!--Disable the automatic generation of work items when the build fails-->
    <SkipWorkItemCreation>true</SkipWorkItemCreation>
    
    <TransformersBin>$(BinariesRoot)\Release</TransformersBin>
  </PropertyGroup>
  
  
  <!-- Define the sequence of custom steps that will be defined in the build
       definition specific files.
   -->
  <PropertyGroup>
    <CompileDependsOn>
      $(CompileDependsOn);
    </CompileDependsOn>

    <TestDependsOn>
      $(TestDependsOn);

    </TestDependsOn>
  </PropertyGroup>

  <!--
  ###################################################################################
  Common Build Targets
  ###################################################################################
  -->
  

</Project>